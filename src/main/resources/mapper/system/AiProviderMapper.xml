<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youlai.boot.system.mapper.AiProviderMapper">

    <!-- 获取模型供应商分页列表 -->
    <select id="getAiProviderPage" resultType="com.youlai.boot.system.model.vo.AiProviderVO">
        SELECT
                id,
                provider_name,
                base_url,
                is_enabled,
                create_time,
                update_time
        FROM
            ai_provider
        <where>
            <if test="queryParams.providerName != null and queryParams.providerName != ''">
                AND provider_name LIKE CONCAT('%', #{queryParams.providerName}, '%')
            </if>
        </where>
    </select>

</mapper>
